# Use /dev/null for NUL on Unix and NUL on Windows
NUL="/dev/null"
case "$(uname -s)" in
  CYGWIN*) NUL="NUL" ;;
  MINGW*) NUL="NUL" ;;
  MSYS*) NUL="NUL" ;;
esac
# Determine command line arguments
GRADLE_OPTS="$GRADLE_OPTS -Dorg.gradle.appname=gradlew"
# Collect all arguments in a single variable
ALL_ARGS="$@"
# For Cygwin or MSYS, switch paths to Windows format before running java
if [ "$(uname -o 2>/dev/null || echo "Unknown")" = "Cygwin" ] || [ "$(uname -o 2>/dev/null || echo "Unknown")" = "Msys" ]; then
  CLASSPATH=$(cygpath --path --windows "$CLASSPATH")
fi
# Execute Gradle
exec "$JAVACMD" $GRADLE_OPTS \
  -classpath "$CLASSPATH" \
  -Dgradle.user.home="$GRADLE_USER_HOME" \
  org.gradle.wrapper.GradleWrapperMain \
  "$ALL_ARGS"
